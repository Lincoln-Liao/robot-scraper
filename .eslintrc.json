{
    "extends": ["airbnb-base", ".eslintrc-todo"],
    "env": {
      "mocha": true
    },
    "rules": {
      "comma-dangle": ["error", "never"],
      "max-len": ["error", {"code":120, "comments": 120}],
      "no-template-curly-in-string": "off",
      "no-plusplus": "off",
      "guard-for-in": "off"
    }
}
